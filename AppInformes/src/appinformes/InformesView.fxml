<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appinformes.InformesViewController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Informes">
            <items>
              <MenuItem fx:id="m_facturas" mnemonicParsing="false" onAction="#onAction" text="Listado Facturas" />
                  <MenuItem fx:id="m_ventastotales" mnemonicParsing="false" onAction="#onAction1" text="Ventas Totales" />
                  <MenuItem fx:id="m_facturascliente" mnemonicParsing="false" onAction="#onAction2" text="Facturas por Cliente" />
                  <MenuItem fx:id="m_subinforme" mnemonicParsing="false" onAction="#onAction3" text="Subinforme Listado Facturas" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
